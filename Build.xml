		<Copy SourceFiles="@(CSSTargetFiles)" DestinationFolder="$(Publish_Content_Dir)" SkipUnchangedFiles="true" />
	</Target>

	<!-- 刪除編譯後不需要的文件 -->
	<Target Name="Remove_UnnecessaryDll">
		<ItemGroup>
			<UnnecessaryTargetFiles_Obj Include="
										$(Publish_Bin_Dir)\*.xml;
										$(Publish_Bin_Dir)\*.pdb;
										" />
		</ItemGroup>
		<Delete Files="@(UnnecessaryTargetFiles_Obj)" />
	</Target>

</Project>
