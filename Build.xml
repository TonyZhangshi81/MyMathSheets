<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" DefaultTargets="Publish" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<PropertyGroup>
		<TargetFrameworkVersion>v4.5</TargetFrameworkVersion>
		<!-- 項目工程所在目錄 -->
		<RootDir>$(MSBuildProjectDirectory)</RootDir>
		<!-- 編譯參數指定 -->
		<Build_Configuration>Release</Build_Configuration>
		<!-- 部署位置：Bin文件夾 -->
		<Publish_Bin_Dir>$(RootDir)\Publish\bin</Publish_Bin_Dir>
		<!-- 部署位置：前台運用程序Image文件夾 -->
		<Publish_AppImage_Dir>$(RootDir)\Publish\Content\image</Publish_AppImage_Dir>
		<!-- 部署位置：Config文件夾 -->
		<Publish_Config_Dir>$(RootDir)\Publish\Config</Publish_Config_Dir>
		<!-- 部署位置：HtmlTemplate文件夾 -->
		<Publish_HtmlTemplate_Dir>$(RootDir)\Publish\HtmlTemplate</Publish_HtmlTemplate_Dir>
		<!-- 部署位置：Scripts文件夾 -->
		<Publish_Scripts_Dir>$(RootDir)\Publish\App_Data\Work\Scripts</Publish_Scripts_Dir>
		<!-- 部署位置：Content文件夾 -->
		<Publish_Content_Dir>$(RootDir)\Publish\App_Data\Work\Content</Publish_Content_Dir>
		<!-- 部署位置：Page文件夾 -->
		<Publish_Page_Dir>$(RootDir)\Publish\App_Data\Work\Page</Publish_Page_Dir>
		<!-- 部署位置：Images文件夾 -->
		<Publish_Images_Dir>$(Publish_Content_Dir)\image</Publish_Images_Dir>
		<!-- 部署位置：fonts文件夾 -->
		<Publish_Fonts_Dir>$(Publish_Content_Dir)\fonts</Publish_Fonts_Dir>
		<!-- 部署位置：switcher文件夾 -->
		<Publish_Switcher_Dir>$(Publish_Content_Dir)\switcher</Publish_Switcher_Dir>
		<!-- 部署位置：Games文件夾 -->
		<Publish_Games_Dir>$(RootDir)\Publish\App_Data\Work\Games</Publish_Games_Dir>
		<!-- 部署位置：Plugin文件夾 -->
		<Publish_Plugin_Dir>$(RootDir)\Publish\App_Data\Work\Plugin</Publish_Plugin_Dir>

		<!-- 個編譯項目所在目錄位置 -->
		<Project_CommonLib_Dir>$(RootDir)\CommonLib</Project_CommonLib_Dir>
		<Project_BasicOperationsLib_Dir>$(RootDir)\ComputationalStrategy</Project_BasicOperationsLib_Dir>
		<Project_MathSheetsSettingApp_Dir>$(RootDir)\MathSheetsSettingApp</Project_MathSheetsSettingApp_Dir>
		<Project_TheFormulaShows_Dir>$(RootDir)\TheFormulaShows</Project_TheFormulaShows_Dir>
		<Project_Topic_Arithmetic_Dir>$(RootDir)\ComputationalStrategy\TopicModule\Arithmetic</Project_Topic_Arithmetic_Dir>
		<Project_Show_Arithmetic_Dir>$(RootDir)\TheFormulaShows\DisplayModule\Arithmetic</Project_Show_Arithmetic_Dir>
		<Project_Topic_CombinatorialEquation_Dir>$(RootDir)\ComputationalStrategy\TopicModule\CombinatorialEquation</Project_Topic_CombinatorialEquation_Dir>
		<Project_Show_CombinatorialEquation_Dir>$(RootDir)\TheFormulaShows\DisplayModule\CombinatorialEquation</Project_Show_CombinatorialEquation_Dir>
		<Project_Topic_ComputingConnection_Dir>$(RootDir)\ComputationalStrategy\TopicModule\ComputingConnection</Project_Topic_ComputingConnection_Dir>
		<Project_Show_ComputingConnection_Dir>$(RootDir)\TheFormulaShows\DisplayModule\ComputingConnection</Project_Show_ComputingConnection_Dir>
		<Project_Topic_EqualityComparison_Dir>$(RootDir)\ComputationalStrategy\TopicModule\EqualityComparison</Project_Topic_EqualityComparison_Dir>
		<Project_Show_EqualityComparison_Dir>$(RootDir)\TheFormulaShows\DisplayModule\EqualityComparison</Project_Show_EqualityComparison_Dir>
		<Project_Topic_EqualityLinkage_Dir>$(RootDir)\ComputationalStrategy\TopicModule\EqualityLinkage</Project_Topic_EqualityLinkage_Dir>
		<Project_Show_EqualityLinkage_Dir>$(RootDir)\TheFormulaShows\DisplayModule\EqualityLinkage</Project_Show_EqualityLinkage_Dir>
		<Project_Topic_FindNearestNumber_Dir>$(RootDir)\ComputationalStrategy\TopicModule\FindNearestNumber</Project_Topic_FindNearestNumber_Dir>
		<Project_Show_FindNearestNumber_Dir>$(RootDir)\TheFormulaShows\DisplayModule\FindNearestNumber</Project_Show_FindNearestNumber_Dir>
		<Project_Topic_FindTheLaw_Dir>$(RootDir)\ComputationalStrategy\TopicModule\FindTheLaw</Project_Topic_FindTheLaw_Dir>
		<Project_Show_FindTheLaw_Dir>$(RootDir)\TheFormulaShows\DisplayModule\FindTheLaw</Project_Show_FindTheLaw_Dir>
		<Project_Topic_CurrencyLinkage_Dir>$(RootDir)\ComputationalStrategy\TopicModule\CurrencyLinkage</Project_Topic_CurrencyLinkage_Dir>
		<Project_Show_CurrencyLinkage_Dir>$(RootDir)\TheFormulaShows\DisplayModule\CurrencyLinkage</Project_Show_CurrencyLinkage_Dir>
		<Project_Topic_CurrencyOperation_Dir>$(RootDir)\ComputationalStrategy\TopicModule\CurrencyOperation</Project_Topic_CurrencyOperation_Dir>
		<Project_Show_CurrencyOperation_Dir>$(RootDir)\TheFormulaShows\DisplayModule\CurrencyOperation</Project_Show_CurrencyOperation_Dir>
		<Project_Topic_FruitsLinkage_Dir>$(RootDir)\ComputationalStrategy\TopicModule\FruitsLinkage</Project_Topic_FruitsLinkage_Dir>
		<Project_Show_FruitsLinkage_Dir>$(RootDir)\TheFormulaShows\DisplayModule\FruitsLinkage</Project_Show_FruitsLinkage_Dir>
		<Project_Topic_GapFillingProblems_Dir>$(RootDir)\ComputationalStrategy\TopicModule\GapFillingProblems</Project_Topic_GapFillingProblems_Dir>
		<Project_Show_GapFillingProblems_Dir>$(RootDir)\TheFormulaShows\DisplayModule\GapFillingProblems</Project_Show_GapFillingProblems_Dir>
		<Project_Topic_HowMuchMore_Dir>$(RootDir)\ComputationalStrategy\TopicModule\HowMuchMore</Project_Topic_HowMuchMore_Dir>
		<Project_Show_HowMuchMore_Dir>$(RootDir)\TheFormulaShows\DisplayModule\HowMuchMore</Project_Show_HowMuchMore_Dir>
		<Project_Topic_LearnCurrency_Dir>$(RootDir)\ComputationalStrategy\TopicModule\LearnCurrency</Project_Topic_LearnCurrency_Dir>
		<Project_Show_LearnCurrency_Dir>$(RootDir)\TheFormulaShows\DisplayModule\LearnCurrency</Project_Show_LearnCurrency_Dir>
		<Project_Topic_LearnLengthUnit_Dir>$(RootDir)\ComputationalStrategy\TopicModule\LearnLengthUnit</Project_Topic_LearnLengthUnit_Dir>
		<Project_Show_LearnLengthUnit_Dir>$(RootDir)\TheFormulaShows\DisplayModule\LearnLengthUnit</Project_Show_LearnLengthUnit_Dir>
		<Project_Topic_MathUpright_Dir>$(RootDir)\ComputationalStrategy\TopicModule\MathUpright</Project_Topic_MathUpright_Dir>
		<Project_Show_MathUpright_Dir>$(RootDir)\TheFormulaShows\DisplayModule\MathUpright</Project_Show_MathUpright_Dir>
		<Project_Topic_MathWordProblems_Dir>$(RootDir)\ComputationalStrategy\TopicModule\MathWordProblems</Project_Topic_MathWordProblems_Dir>
		<Project_Show_MathWordProblems_Dir>$(RootDir)\TheFormulaShows\DisplayModule\MathWordProblems</Project_Show_MathWordProblems_Dir>
		<Project_Topic_NumericSorting_Dir>$(RootDir)\ComputationalStrategy\TopicModule\NumericSorting</Project_Topic_NumericSorting_Dir>
		<Project_Show_NumericSorting_Dir>$(RootDir)\TheFormulaShows\DisplayModule\NumericSorting</Project_Show_NumericSorting_Dir>
		<Project_Topic_SchoolClock_Dir>$(RootDir)\ComputationalStrategy\TopicModule\SchoolClock</Project_Topic_SchoolClock_Dir>
		<Project_Show_SchoolClock_Dir>$(RootDir)\TheFormulaShows\DisplayModule\SchoolClock</Project_Show_SchoolClock_Dir>
		<Project_Topic_ScoreGoal_Dir>$(RootDir)\ComputationalStrategy\TopicModule\ScoreGoal</Project_Topic_ScoreGoal_Dir>
		<Project_Show_ScoreGoal_Dir>$(RootDir)\TheFormulaShows\DisplayModule\ScoreGoal</Project_Show_ScoreGoal_Dir>
		<Project_Topic_TimeCalculation_Dir>$(RootDir)\ComputationalStrategy\TopicModule\TimeCalculation</Project_Topic_TimeCalculation_Dir>
		<Project_Show_TimeCalculation_Dir>$(RootDir)\TheFormulaShows\DisplayModule\TimeCalculation</Project_Show_TimeCalculation_Dir>

		<!-- 個編譯項目的工程文件 -->
		<Project_CommonLib>$(Project_CommonLib_Dir)\MyMathSheets.CommonLib.csproj</Project_CommonLib>
		<Project_BasicOperationsLib>$(Project_BasicOperationsLib_Dir)\MyMathSheets.BasicOperationsLib.csproj</Project_BasicOperationsLib>
		<Project_MathSheetsSettingApp>$(Project_MathSheetsSettingApp_Dir)\MyMathSheets.MathSheetsSettingApp.csproj</Project_MathSheetsSettingApp>
		<Project_Topic_Arithmetic>$(Project_Topic_Arithmetic_Dir)\MyMathSheets.ComputationalStrategy.Arithmetic.csproj</Project_Topic_Arithmetic>
		<Project_Show_Arithmetic>$(Project_Show_Arithmetic_Dir)\MyMathSheets.TheFormulaShows.Arithmetic.csproj</Project_Show_Arithmetic>
		<Project_Topic_CombinatorialEquation>$(Project_Topic_CombinatorialEquation_Dir)\MyMathSheets.ComputationalStrategy.CombinatorialEquation.csproj</Project_Topic_CombinatorialEquation>
		<Project_Show_CombinatorialEquation>$(Project_Show_CombinatorialEquation_Dir)\MyMathSheets.TheFormulaShows.CombinatorialEquation.csproj</Project_Show_CombinatorialEquation>
		<Project_Topic_ComputingConnection>$(Project_Topic_ComputingConnection_Dir)\MyMathSheets.ComputationalStrategy.ComputingConnection.csproj</Project_Topic_ComputingConnection>
		<Project_Show_ComputingConnection>$(Project_Show_ComputingConnection_Dir)\MyMathSheets.TheFormulaShows.ComputingConnection.csproj</Project_Show_ComputingConnection>
		<Project_Topic_EqualityComparison>$(Project_Topic_EqualityComparison_Dir)\MyMathSheets.ComputationalStrategy.EqualityComparison.csproj</Project_Topic_EqualityComparison>
		<Project_Show_EqualityComparison>$(Project_Show_EqualityComparison_Dir)\MyMathSheets.TheFormulaShows.EqualityComparison.csproj</Project_Show_EqualityComparison>
		<Project_Topic_EqualityLinkage>$(Project_Topic_EqualityLinkage_Dir)\MyMathSheets.ComputationalStrategy.EqualityLinkage.csproj</Project_Topic_EqualityLinkage>
		<Project_Show_EqualityLinkage>$(Project_Show_EqualityLinkage_Dir)\MyMathSheets.TheFormulaShows.EqualityLinkage.csproj</Project_Show_EqualityLinkage>
		<Project_Topic_FindNearestNumber>$(Project_Topic_FindNearestNumber_Dir)\MyMathSheets.ComputationalStrategy.FindNearestNumber.csproj</Project_Topic_FindNearestNumber>
		<Project_Show_FindNearestNumber>$(Project_Show_FindNearestNumber_Dir)\MyMathSheets.TheFormulaShows.FindNearestNumber.csproj</Project_Show_FindNearestNumber>
		<Project_Topic_FindTheLaw>$(Project_Topic_FindTheLaw_Dir)\MyMathSheets.ComputationalStrategy.FindTheLaw.csproj</Project_Topic_FindTheLaw>
		<Project_Show_FindTheLaw>$(Project_Show_FindTheLaw_Dir)\MyMathSheets.TheFormulaShows.FindTheLaw.csproj</Project_Show_FindTheLaw>
		<Project_Topic_CurrencyLinkage>$(Project_Topic_CurrencyLinkage_Dir)\MyMathSheets.ComputationalStrategy.CurrencyLinkage.csproj</Project_Topic_CurrencyLinkage>
		<Project_Show_CurrencyLinkage>$(Project_Show_CurrencyLinkage_Dir)\MyMathSheets.TheFormulaShows.CurrencyLinkage.csproj</Project_Show_CurrencyLinkage>
		<Project_Topic_CurrencyOperation>$(Project_Topic_CurrencyOperation_Dir)\MyMathSheets.ComputationalStrategy.CurrencyOperation.csproj</Project_Topic_CurrencyOperation>
		<Project_Show_CurrencyOperation>$(Project_Show_CurrencyOperation_Dir)\MyMathSheets.TheFormulaShows.CurrencyOperation.csproj</Project_Show_CurrencyOperation>
		<Project_Topic_FruitsLinkage>$(Project_Topic_FruitsLinkage_Dir)\MyMathSheets.ComputationalStrategy.FruitsLinkage.csproj</Project_Topic_FruitsLinkage>
		<Project_Show_FruitsLinkage>$(Project_Show_FruitsLinkage_Dir)\MyMathSheets.TheFormulaShows.FruitsLinkage.csproj</Project_Show_FruitsLinkage>
		<Project_Topic_GapFillingProblems>$(Project_Topic_GapFillingProblems_Dir)\MyMathSheets.ComputationalStrategy.GapFillingProblems.csproj</Project_Topic_GapFillingProblems>
		<Project_Show_GapFillingProblems>$(Project_Show_GapFillingProblems_Dir)\MyMathSheets.TheFormulaShows.GapFillingProblems.csproj</Project_Show_GapFillingProblems>
		<Project_Topic_HowMuchMore>$(Project_Topic_HowMuchMore_Dir)\MyMathSheets.ComputationalStrategy.HowMuchMore.csproj</Project_Topic_HowMuchMore>
		<Project_Show_HowMuchMore>$(Project_Show_HowMuchMore_Dir)\MyMathSheets.TheFormulaShows.HowMuchMore.csproj</Project_Show_HowMuchMore>
		<Project_Topic_LearnCurrency>$(Project_Topic_LearnCurrency_Dir)\MyMathSheets.ComputationalStrategy.LearnCurrency.csproj</Project_Topic_LearnCurrency>
		<Project_Show_LearnCurrency>$(Project_Show_LearnCurrency_Dir)\MyMathSheets.TheFormulaShows.LearnCurrency.csproj</Project_Show_LearnCurrency>
		<Project_Topic_LearnLengthUnit>$(Project_Topic_LearnLengthUnit_Dir)\MyMathSheets.ComputationalStrategy.LearnLengthUnit.csproj</Project_Topic_LearnLengthUnit>
		<Project_Show_LearnLengthUnit>$(Project_Show_LearnLengthUnit_Dir)\MyMathSheets.TheFormulaShows.LearnLengthUnit.csproj</Project_Show_LearnLengthUnit>
		<Project_Topic_MathUpright>$(Project_Topic_MathUpright_Dir)\MyMathSheets.ComputationalStrategy.MathUpright.csproj</Project_Topic_MathUpright>
		<Project_Show_MathUpright>$(Project_Show_MathUpright_Dir)\MyMathSheets.TheFormulaShows.MathUpright.csproj</Project_Show_MathUpright>
		<Project_Topic_MathWordProblems>$(Project_Topic_MathWordProblems_Dir)\MyMathSheets.ComputationalStrategy.MathWordProblems.csproj</Project_Topic_MathWordProblems>
		<Project_Show_MathWordProblems>$(Project_Show_MathWordProblems_Dir)\MyMathSheets.TheFormulaShows.MathWordProblems.csproj</Project_Show_MathWordProblems>
		<Project_Topic_NumericSorting>$(Project_Topic_NumericSorting_Dir)\MyMathSheets.ComputationalStrategy.NumericSorting.csproj</Project_Topic_NumericSorting>
		<Project_Show_NumericSorting>$(Project_Show_NumericSorting_Dir)\MyMathSheets.TheFormulaShows.NumericSorting.csproj</Project_Show_NumericSorting>
		<Project_Topic_SchoolClock>$(Project_Topic_SchoolClock_Dir)\MyMathSheets.ComputationalStrategy.SchoolClock.csproj</Project_Topic_SchoolClock>
		<Project_Show_SchoolClock>$(Project_Show_SchoolClock_Dir)\MyMathSheets.TheFormulaShows.SchoolClock.csproj</Project_Show_SchoolClock>
		<Project_Topic_ScoreGoal>$(Project_Topic_ScoreGoal_Dir)\MyMathSheets.ComputationalStrategy.ScoreGoal.csproj</Project_Topic_ScoreGoal>
		<Project_Show_ScoreGoal>$(Project_Show_ScoreGoal_Dir)\MyMathSheets.TheFormulaShows.ScoreGoal.csproj</Project_Show_ScoreGoal>
		<Project_Topic_TimeCalculation>$(Project_Topic_TimeCalculation_Dir)\MyMathSheets.ComputationalStrategy.TimeCalculation.csproj</Project_Topic_TimeCalculation>
		<Project_Show_TimeCalculation>$(Project_Show_TimeCalculation_Dir)\MyMathSheets.TheFormulaShows.TimeCalculation.csproj</Project_Show_TimeCalculation>

	</PropertyGroup>

	<Target Name="Publish">
		<!-- 編譯結果刪除 -->
		<RemoveDir Directories="$(Publish_Bin_Dir)"/>

		<!-- 創建部署所需的文件目錄 -->
		<MakeDir Directories="
				 $(Publish_Bin_Dir);
				 $(Publish_Config_Dir);
				 $(Publish_Scripts_Dir)\Ext;
				 $(Publish_Images_Dir);
				 $(Publish_Page_Dir);
				 $(Publish_HtmlTemplate_Dir);
				 $(Publish_Games_Dir);
				 "/>

		<!-- 編譯項目依次執行 -->
		<CallTarget Targets="Build_CommonLib" />
		<CallTarget Targets="Build_BasicOperationsLib" />
		<CallTarget Targets="Build_FormulaModuleLib" />
		<CallTarget Targets="Build_MathSheetsSettingApp" />

		<!-- 資源部署 -->
		<CallTarget Targets="Copy_Resources" />
		<!-- 游戏部署 -->
		<CallTarget Targets="Copy_Game_Resources" />
		<!-- 插件部署 -->
		<CallTarget Targets="Copy_Plugin_Resources" />

		<!-- 刪除編譯後不需要的文件 -->
		<CallTarget Targets="Remove_UnnecessaryDll" />
	</Target>

	<!-- 插件部署 -->
	<Target Name="Copy_Plugin_Resources">
		<!-- 蝴蝶 -->
		<CallTarget Targets="Copy_Plugin_Butterfly" />
		<!-- 虛擬人物 -->
		<CallTarget Targets="Copy_Plugin_TonyShakeHead" />
	</Target>

	<Target Name="Copy_Plugin_Butterfly">
		<ItemGroup>
			<ButterflyHtmlTargetFiles Include="$(Project_TheFormulaShows_Dir)\Plugin\Butterfly\*.html" />
			<ButterflyCssTargetFiles Include="$(Project_TheFormulaShows_Dir)\Plugin\Butterfly\**\*.css" />
		</ItemGroup>
		<Copy SourceFiles="@(ButterflyHtmlTargetFiles)" DestinationFolder="$(Publish_Plugin_Dir)\Butterfly" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(ButterflyCssTargetFiles)" DestinationFolder="$(Publish_Plugin_Dir)\Butterfly\css" SkipUnchangedFiles="true" />
	</Target>

	<Target Name="Copy_Plugin_TonyShakeHead">
		<ItemGroup>
			<TonyShakeHeadHtmlTargetFiles Include="$(Project_TheFormulaShows_Dir)\Plugin\TonyShakeHead\*.html" />
			<TonyShakeHeadCssTargetFiles Include="$(Project_TheFormulaShows_Dir)\Plugin\TonyShakeHead\**\*.css" />
		</ItemGroup>
		<Copy SourceFiles="@(TonyShakeHeadHtmlTargetFiles)" DestinationFolder="$(Publish_Plugin_Dir)\TonyShakeHead" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(TonyShakeHeadCssTargetFiles)" DestinationFolder="$(Publish_Plugin_Dir)\TonyShakeHead\css" SkipUnchangedFiles="true" />
	</Target>

	<!-- 游戏部署 -->
	<Target Name="Copy_Game_Resources">
		<!-- 打方塊 -->
		<CallTarget Targets="Copy_Game_BreakTheBricks" />
		<!-- 貪吃蛇 -->
		<CallTarget Targets="Copy_Game_Snake" />
		<!-- 21張牌猜牌 -->
		<CallTarget Targets="Copy_Game_Poker" />
	</Target>

	<Target Name="Copy_Game_BreakTheBricks">
		<ItemGroup>
			<BreakTheBricksHtmlTargetFiles Include="$(Project_TheFormulaShows_Dir)\Games\break the bricks\*.html" />
			<BreakTheBricksCssTargetFiles Include="$(Project_TheFormulaShows_Dir)\Games\break the bricks\**\*.css" />
			<BreakTheBricksFontsTargetFiles Include="$(Project_TheFormulaShows_Dir)\Games\break the bricks\fonts\*.*" />
			<BreakTheBricksJsTargetFiles Include="$(Project_TheFormulaShows_Dir)\Games\break the bricks\**\*.js" />
		</ItemGroup>
		<Copy SourceFiles="@(BreakTheBricksHtmlTargetFiles)" DestinationFolder="$(Publish_Games_Dir)\break the bricks" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(BreakTheBricksCssTargetFiles)" DestinationFolder="$(Publish_Games_Dir)\break the bricks\css" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(BreakTheBricksFontsTargetFiles)" DestinationFolder="$(Publish_Games_Dir)\break the bricks\fonts" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(BreakTheBricksJsTargetFiles)" DestinationFolder="$(Publish_Games_Dir)\break the bricks\js" SkipUnchangedFiles="true" />
	</Target>

	<Target Name="Copy_Game_Snake">
		<ItemGroup>
			<SnakeHtmlTargetFiles Include="$(Project_TheFormulaShows_Dir)\Games\snake\*.*" />
		</ItemGroup>
		<Copy SourceFiles="@(SnakeHtmlTargetFiles)" DestinationFolder="$(Publish_Games_Dir)\snake" SkipUnchangedFiles="true" />
	</Target>
	
	<Target Name="Copy_Game_Poker">
		<ItemGroup>
			<PokerHtmlTargetFiles Include="$(Project_TheFormulaShows_Dir)\Games\poker\*.html" />
			<PokerCssTargetFiles Include="$(Project_TheFormulaShows_Dir)\Games\poker\**\*.css" />
			<PokerImagesTargetFiles Include="$(Project_TheFormulaShows_Dir)\Games\poker\image\*.*" />
			<PokerJsTargetFiles Include="$(Project_TheFormulaShows_Dir)\Games\poker\**\*.js" />
		</ItemGroup>
		<Copy SourceFiles="@(PokerHtmlTargetFiles)" DestinationFolder="$(Publish_Games_Dir)\poker" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(PokerCssTargetFiles)" DestinationFolder="$(Publish_Games_Dir)\poker\css" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(PokerImagesTargetFiles)" DestinationFolder="$(Publish_Games_Dir)\poker\image" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(PokerJsTargetFiles)" DestinationFolder="$(Publish_Games_Dir)\poker\js" SkipUnchangedFiles="true" />
	</Target>
	
	<!-- 編譯項目詳細設定 -->
	<Target Name="Build_CommonLib">
		<MSBuild Projects="$(Project_CommonLib)" Properties="Configuration=$(Build_Configuration);OutputPath=$(Publish_Bin_Dir);" Targets="Clean;Rebuild" BuildInParallel="$(BuildInParallel)" />
	</Target>

	<Target Name="Build_BasicOperationsLib">
		<MSBuild Projects="$(Project_BasicOperationsLib)" Properties="Configuration=$(Build_Configuration);OutputPath=$(Publish_Bin_Dir);" Targets="Clean;Rebuild" BuildInParallel="$(BuildInParallel)" />
	</Target>

	<!-- 前台運用程序 -->
	<Target Name="Build_MathSheetsSettingApp">
		<MSBuild Projects="$(Project_MathSheetsSettingApp)" Properties="Configuration=$(Build_Configuration);OutputPath=$(Publish_Bin_Dir);" Targets="Clean;Rebuild" BuildInParallel="$(BuildInParallel)" />

		<ItemGroup>
			<AppContentImageTargetFiles Include="$(Project_MathSheetsSettingApp_Dir)\Content\**\*.png" />
			<TopicManagementTargetFiles Include="$(Project_MathSheetsSettingApp_Dir)\Config\TopicManagement.json" />
		</ItemGroup>
		<Copy SourceFiles="@(AppContentImageTargetFiles)" DestinationFolder="$(Publish_AppImage_Dir)" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(TopicManagementTargetFiles)" DestinationFolder="$(Publish_Config_Dir)" SkipUnchangedFiles="true" />
	</Target>

	<!-- 資源部署 -->
	<Target Name="Copy_Resources">
		<ItemGroup>
			<!-- 字體文件 -->
			<FontsTargetFiles Include="$(Project_TheFormulaShows_Dir)\Content\fonts\*.*" />
			<!-- 圖片文件 -->
			<ImageTargetFiles Include="$(Project_TheFormulaShows_Dir)\Content\image\honor\*.png">
				<Project>TheFormulaShows-honor</Project>
			</ImageTargetFiles>
			<ImageTargetFiles Include="$(Project_TheFormulaShows_Dir)\Content\image\*.png;$(Project_TheFormulaShows_Dir)\Content\image\*.ico">
				<Project>TheFormulaShows-image</Project>
			</ImageTargetFiles>
			<!-- 主題文件 -->
			<SwitcherTargetFiles Include="$(Project_TheFormulaShows_Dir)\Content\switcher\*.*" />
			<!-- 樣式文件 -->
			<CommonCSSTargetFiles Include="
							$(Project_TheFormulaShows_Dir)\Content\bootstrap.min.css;
							$(Project_TheFormulaShows_Dir)\Content\default.css;
							$(Project_TheFormulaShows_Dir)\Content\font-awesome.min.css;
							$(Project_TheFormulaShows_Dir)\Content\Shake.css;
							" />
			<!-- 腳本文件 -->
			<CommonJSTargetFiles Include="
							$(Project_TheFormulaShows_Dir)\Scripts\bootstrap.min.js;
							$(Project_TheFormulaShows_Dir)\Scripts\jquery.base64.js;
							$(Project_TheFormulaShows_Dir)\Scripts\jquery.easing.1.3.js;
							$(Project_TheFormulaShows_Dir)\Scripts\jquery.easyui.min.js;
							$(Project_TheFormulaShows_Dir)\Scripts\jquery.linq.min.js;
							$(Project_TheFormulaShows_Dir)\Scripts\jquery.PrintArea.js;
							$(Project_TheFormulaShows_Dir)\Scripts\jquery.rating-stars.min.js;
							$(Project_TheFormulaShows_Dir)\Scripts\jquery-3.3.1.min.js;
							$(Project_TheFormulaShows_Dir)\Scripts\json2.min.js;
							$(Project_TheFormulaShows_Dir)\Scripts\store.legacy.min.js;
							">
				<Project>TheFormulaShows-base-js</Project>
			</CommonJSTargetFiles>
			<CommonJSTargetFiles Include="
							$(Project_TheFormulaShows_Dir)\Scripts\Ext\MathSheets.Common.js;
							$(Project_TheFormulaShows_Dir)\Scripts\Ext\MathSheets.HelloMrTony.js;
							">
				<Project>TheFormulaShows-ext-js</Project>
			</CommonJSTargetFiles>
			<CommonJSTargetFiles Include="$(Project_TheFormulaShows_Dir)\Scripts\snap\snap.svg-min.js">
				<Project>TheFormulaShows-snap-js</Project>
			</CommonJSTargetFiles>
			<CommonJSTargetFiles Include="$(Project_TheFormulaShows_Dir)\Scripts\umd\popper.js">
				<Project>TheFormulaShows-umd-js</Project>
			</CommonJSTargetFiles>
			<!-- 模板文件 -->
			<HtmlTemplateTargetFiles Include="$(Project_TheFormulaShows_Dir)\Template\HTMLPage" />

		</ItemGroup>
		<Copy SourceFiles="@(FontsTargetFiles)" DestinationFolder="$(Publish_Fonts_Dir)" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(ImageTargetFiles)" Condition="'%(Project)'=='TheFormulaShows-honor'" DestinationFolder="$(Publish_Images_Dir)\honor" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(ImageTargetFiles)" Condition="'%(Project)'=='TheFormulaShows-image'" DestinationFolder="$(Publish_Images_Dir)" SkipUnchangedFiles="true" />
		<Copy SourceFiles="@(SwitcherTargetFiles)" DestinationFolder="$(Publish_Switcher_Dir)" SkipUnchangedFiles="true" />
