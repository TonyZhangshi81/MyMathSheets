	<script>
		// 打印
		function btnPrintClick() {
			//<!--ARITHMETICPRINTSETTING-->
			//<!--EQUALITYCOMPARISONPRINTSETTING-->
			//<!--COMPUTINGCONNECTIONPRINTSETTING-->
			//<!--MATHWORDPROBLEMSPRINTSETTING-->
			//<!--FINDNEARESTNUMBERPRINTSETTING-->
			//<!--COMBINATORIALEQUATIONPRINTSETTING-->
			//<!--HOWMUCHMOREPRINTSETTING-->
			//<!--FINDTHELAWPRINTSETTING-->
			//<!--NUMERICSORTINGPRINTSETTING-->
			//<!--LEARNCURRENCYPRINTSETTING-->
			//<!--EQUALITYLINKAGEPRINTSETTING-->
			//<!--SCHOOLCLOCKPRINTSETTING-->
			//<!--CURRENCYOPERATIONPRINTSETTING-->
			//<!--CURRENCYLINKAGEPRINTSETTING-->
			//<!--TIMECALCULATIONPRINTSETTING-->
			//<!--LEARNLENGTHUNITPRINTSETTING-->

			// 頁面答應處理
			MathSheets.Common.pagePrint('divPrintContent');
			// 打印后恢復頁面最初狀態設置
			MathSheets.Common.printAfterSetting('btnTheirPapers', 'btnMakeCorrections', 'btnOver');
			// 打印后頁面設定
			//<!--ARITHMETICPRINTAFTERSETTING-->
			//<!--EQUALITYCOMPARISONPRINTAFTERSETTING-->
			//<!--COMPUTINGCONNECTIONPRINTAFTERSETTING-->
			//<!--MATHWORDPROBLEMSPRINTAFTERSETTING-->
			//<!--FINDNEARESTNUMBERPRINTAFTERSETTING-->
			//<!--COMBINATORIALEQUATIONPRINTAFTERSETTING-->
			//<!--HOWMUCHMOREPRINTAFTERSETTING-->
			//<!--FINDTHELAWPRINTAFTERSETTING-->
			//<!--NUMERICSORTINGPRINTAFTERSETTING-->
			//<!--LEARNCURRENCYPRINTAFTERSETTING-->
			//<!--EQUALITYLINKAGEPRINTAFTERSETTING-->
			//<!--SCHOOLCLOCKPRINTAFTERSETTING-->
			//<!--CURRENCYOPERATIONPRINTAFTERSETTING-->
			//<!--CURRENCYLINKAGEPRINTAFTERSETTING-->
			//<!--TIMECALCULATIONPRINTAFTERSETTING-->
			//<!--LEARNLENGTHUNITPRINTAFTERSETTING-->
		}

		// 答題準備按鍵
		function btnReadyClick() {
			// 設定頁面所有輸入域為可用狀態(共通)-交卷按鈕顯示,準備按鈕隱藏,打印按鈕隱藏
			MathSheets.Common.ready('btnTheirPapers', 'btnReady', 'btnPrint');
			// 設定頁面所有輸入域為可用狀態（四則運算）
			//<!--ARITHMETICREADY-->
			// 設定頁面所有輸入域為可用狀態(運算比大小)
			//<!--EQUALITYCOMPARISONREADY-->
			// 設定頁面所有輸入域為可用狀態(等式接龍)
			//<!--COMPUTINGCONNECTIONREADY-->
			// 設定頁面所有輸入域為可用狀態(算數應用題)
			//<!--MATHWORDPROBLEMSREADY-->
			// 設定頁面所有輸入域為可用狀態(水果連連看)
			//<!--FRUITSLINKAGEREADY-->
			// 設定頁面所有輸入域為可用狀態(找到最近的數字)
			//<!--FINDNEARESTNUMBERREADY-->
			// 設定頁面所有輸入域為可用狀態(算式組合)
			//<!--COMBINATORIALEQUATIONREADY-->
			// 設定頁面所有輸入域為可用狀態(射門得分)
			//<!--SCOREGOALREADY-->
			// 設定頁面所有輸入域為可用狀態(比多少)
			//<!--HOWMUCHMOREREADY-->
			// 設定頁面所有輸入域為可用狀態(找規律)
			//<!--FINDTHELAWREADY-->
			// 設定頁面所有輸入域為可用狀態(數字排序)
			//<!--NUMERICSORTINGREADY-->
			// 設定頁面所有輸入域為可用狀態(認識貨幣)
			//<!--LEARNCURRENCYREADY-->
			// 設定頁面所有輸入域為可用狀態(算式連一連)
			//<!--EQUALITYLINKAGEREADY-->
			// 設定頁面所有輸入域為可用狀態(時鐘學習板)
			//<!--SCHOOLCLOCKREADY-->
			// 設定頁面所有輸入域為可用狀態(貨幣運算)
			//<!--CURRENCYOPERATIONREADY-->
			// 設定頁面所有輸入域為可用狀態(認識貨幣)
			//<!--CURRENCYLINKAGEREADY-->
			// 設定頁面所有輸入域為可用狀態(時間運算)
			//<!--TIMECALCULATIONREADY-->
			// 設定頁面所有輸入域為可用狀態(認識長度單位)
			//<!--LEARNLENGTHUNITREADY-->

			// 計時開始
			MathSheets.Common.startTime('spanSS', 'spanMM', 'spanHH');
		}

		// 完成(關閉頁面)
		function btnOverClick() {
			MathSheets.Common.windowClose();
		}

		// 訂正按鈕(不計入答題情況信息)
		function btnMakeCorrectionsClick() {
			var fault = 0;
			// 四則運算題
			//<!--ARITHMETICMAKECORRECTIONS-->
			// 算式比較大小
			//<!--EQUALITYCOMPARISONMAKECORRECTIONS-->
			// 等式接龍
			//<!--COMPUTINGCONNECTIONMAKECORRECTIONS-->
			// 算數應用題
			//<!--MATHWORDPROBLEMSMAKECORRECTIONS-->
			// 水果連連看
			//<!--FRUITSLINKAGEMAKECORRECTIONS-->
			// 找到最近的數字
			//<!--FINDNEARESTNUMBERMAKECORRECTIONS-->
			// 算式組合
			//<!--COMBINATORIALEQUATIONMAKECORRECTIONS-->
			// 射門得分
			//<!--SCOREGOALMAKECORRECTIONS-->
			// 比多少
			//<!--HOWMUCHMOREMAKECORRECTIONS-->
			// 找規律
			//<!--FINDTHELAWMAKECORRECTIONS-->
			// 數字排序
			//<!--NUMERICSORTINGMAKECORRECTIONS-->
			// 認識貨幣
			//<!--LEARNCURRENCYMAKECORRECTIONS-->
			// 算式連一連
			//<!--EQUALITYLINKAGEMAKECORRECTIONS-->
			// 時鐘學習板
			//<!--SCHOOLCLOCKMAKECORRECTIONS-->
			// 貨幣運算
			//<!--CURRENCYOPERATIONMAKECORRECTIONS-->
			// 認識貨幣
			//<!--CURRENCYLINKAGEMAKECORRECTIONS-->
			// 時間運算
			//<!--TIMECALCULATIONMAKECORRECTIONS-->
			// 認識長度單位
			//<!--LEARNLENGTHUNITMAKECORRECTIONS-->

			// 訂正按鈕處理
			MathSheets.Common.makeCorrections(fault, 'btnMakeCorrections', 'btnOver');
		}

		// 交卷按鈕(記錄在答題情況下,以便可以看到前一次做題情況)
		function btnTheirPapersClick() {
			// 四則運算交卷
			//<!--ARITHMETICTHEIRPAPERS-->
			// 算式比大小交卷
			//<!--EQUALITYCOMPARISONTHEIRPAPERS-->
			// 等式接龍交卷
			//<!--COMPUTINGCONNECTIONTHEIRPAPERS-->
			// 算數應用題交卷
			//<!--MATHWORDPROBLEMSTHEIRPAPERS-->
			// 水果連連看
			//<!--FRUITSLINKAGETHEIRPAPERS-->
			// 找到最近的數字
			//<!--FINDNEARESTNUMBERTHEIRPAPERS-->
			// 算式組合
			//<!--COMBINATORIALEQUATIONTHEIRPAPERS-->
			// 射門得分
			//<!--SCOREGOALTHEIRPAPERS-->
			// 比多少
			//<!--HOWMUCHMORETHEIRPAPERS-->
			// 找規律
			//<!--FINDTHELAWTHEIRPAPERS-->
			// 數字排序
			//<!--NUMERICSORTINGTHEIRPAPERS-->
			// 認識貨幣
			//<!--LEARNCURRENCYTHEIRPAPERS-->
			// 算式連一連
			//<!--EQUALITYLINKAGETHEIRPAPERS-->
			// 時鐘學習板
			//<!--SCHOOLCLOCKTHEIRPAPERS-->
			// 貨幣運算
			//<!--CURRENCYOPERATIONTHEIRPAPERS-->
			// 認識貨幣
			//<!--CURRENCYLINKAGETHEIRPAPERS-->
			// 時間運算
			//<!--TIMECALCULATIONTHEIRPAPERS-->
			// 認識長度單位
			//<!--LEARNLENGTHUNITTHEIRPAPERS-->

			// 交卷按鈕處理
			MathSheets.Common.theirPapers('spanOK', 'spanNo', 'btnMakeCorrections', 'btnOver', 'btnTheirPapers');
			// 計時停止（答題結果設定）
			MathSheets.Common.timeStop('spanSS', 'spanMM', 'spanHH');
		}
	</script>
